extends ./layout/layout

block title
  title Quiz board

block content
  button.start-quiz-btn Start quiz
  table.participants-list(data-count="#{countOfTasks}")
    thead
      tr
        th Participant
        - var i = 1
        while i <= countOfTasks
          th Task #{i}
          - i++
    tbody

block scripts
  script(src="scripts/main.js")
